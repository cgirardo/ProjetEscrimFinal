<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teamcs.controller.AccueilController">
    <children>
        <MenuBar layoutY="1.0" prefHeight="30.0" prefWidth="600.0">
            <menus>
                <Menu mnemonicParsing="false" text="Profil">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#itemProfilEditerAction" text="Editer" />
                        <MenuItem mnemonicParsing="false" onAction="#itemProfilDeconnecterAction" text="Se déconnecter" />
                        <MenuItem mnemonicParsing="false" onAction="#itemProfilQuitterAction" text="Quitter" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Médicaments">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#itemMedicamentsRechercherAction" text="Rechercher" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Missions">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#itemMissionsConsulterAction" text="Consulter" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
   </children>
</AnchorPane>
